        let currentMenu = [];
        let selectedActivity = {};

        function updateMenu(title, level) {
            console.log("updateMenu chamado com título:", title, "nível:", level);

            let menuItems = document.getElementById('menuItems');
            menuItems.innerHTML = '';

            const titleElement = document.createElement('div');
            titleElement.classList.add('menu-title');
            titleElement.innerText = title;
            menuItems.appendChild(titleElement);

            let submenus = [];

            if (conferencias[title]) {
                console.log("Carregando conferências para:", title);
                submenus = conferencias[title];
            } else if (submenusEspecificos[title]) {
                console.log("Carregando submenus específicos para:", title);
                submenus = submenusEspecificos[title];
            } else if (atividades[title]) {
                console.log("Carregando atividades para:", title);
                submenus = atividades[title];
                // Achou atividades para o título; precisamos lidar com a subestrutura de atividades.
            } else if (level === 1) {
                console.log("Adicionando submenus principais");
                submenus = ['Ekoga', 'Manaus', 'M.S.A', 'Misa', 'Shorewood', 'Tecnopar'];
            } else {
                console.log("Adicionando submenus de atividades específicas");
                submenus = atividades[label];
            }

            console.log("Submenus encontrados:", submenus);

            submenus.forEach(submenu => {
                const submenuItem = document.createElement('div');
                submenuItem.classList.add('menu-item');
                submenuItem.innerText = submenu;

                submenuItem.onclick = () => {
                    if (atividades[title] && atividades[title][submenu]) {
                        console.log(`Atividades específicas para ${submenu} em ${title}`);
                        // Você pode adaptar a lógica aqui para explorar as subcategorias como "Federal", "Estadual", "Municipal"
                        addSubmenusEspecificos(submenu, atividades[title][submenu]);
                    } else if (submenusEspecificos[submenu]) {
                        console.log(`Submenus específicos para ${submenu}`);
                        addSubmenusEspecificos(submenu, submenusEspecificos[submenu]);
                    } else {
                        console.log(`Atualizando menu para submenu: ${submenu}`);
                        updateMenu(submenu, level + 1);
                    }
                };

                menuItems.appendChild(submenuItem);
            });

            menuItems.style.position = 'absolute';
            menuItems.style.top = '80px';
            menuItems.style.left = '10px';
            menuItems.style.display = 'flex';
            menuItems.style.flexDirection = 'column';
        }

        function addSubmenusEspecificos(title, submenusList) {
            let menuItems = document.getElementById('menuItems');
            menuItems.innerHTML = '';

            const titleElement = document.createElement('div');
            titleElement.classList.add('menu-title');
            titleElement.innerText = title;
            menuItems.appendChild(titleElement);

            submenusList.forEach(submenu => {
                const submenuItem = document.createElement('div');
                submenuItem.classList.add('menu-item');
                submenuItem.innerText = submenu;

                submenuItem.onclick = () => {
                    alert(`Clicado em ${submenu} em ${title}`);
                };

                menuItems.appendChild(submenuItem);
            });

            menuItems.style.position = 'absolute';
            menuItems.style.top = '80px';
            menuItems.style.left = '10px';
            menuItems.style.display = 'flex';
            menuItems.style.flexDirection = 'column';
        }

        function toggleMainMenu() {
            const menuItems = document.getElementById('menuItems');
            if (menuItems.style.display === 'none' && currentMenu.length === 0) {
                menuItems.style.display = 'flex';
            } else {
                updateMenu('', 0);
            }
        }

        function showSubmenu(menuItem) {
            const submenu = menuItem.querySelector('.submenu');
            submenu.style.display = 'block';
            const menuName = menuItem.innerText.trim();
            const submenuItems = submenusMap[menuName] || [];
            submenu.style.left = menuItem.offsetWidth + 'px';
            submenu.innerHTML = submenuItems.map(item => `<div class="menu-item" onmouseenter="showSubmenu(this)" onmouseleave="hideSubmenu(this)">${item}</div>`).join('');
        }

        function hideSubmenu(menuItem) {
            const submenu = menuItem.querySelector('.submenu');
            submenu.style.display = 'none';
        }
        function logout() {
        fetch('/logout', {
            method: 'POST',
            headers: {'Content-Type': 'application/json'}
        }).then(response => {
            if (response.redirected) {
                window.location.href = response.url;
            } else {
                alert('Erro ao sair. Tente novamente.');
            }
        }).catch(error => console.error('Erro:', error));
        }
    </script>
