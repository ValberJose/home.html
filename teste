        let currentMenu = [];
        let selectedActivity = {};

    function updateMenu(filial, level, empresa, tipo, tributo) {
        const menuItems = document.getElementById('menuItems');
        menuItems.innerHTML = '';

        if (submenusTributos["Apuração"][tipo] &&
            submenusTributos["Apuração"][tipo][empresa] &&
            submenusTributos["Apuração"][tipo][empresa][filial][tributo]) {

            const tributos = submenusTributos["Apuração"][tipo][empresa][filial][tributo];

            tributos.forEach(tributo => {
                const tributoItem = document.createElement('div');
                tributoItem.classList.add('menu-item');
                tributoItem.innerText = tributo;
                tributoItem.onclick = () => {
                    selectedActivity = {
                        empresa: empresa,
                        filial: filial,
                        tipo: tipo,
                        tributo: tributo
                    };
                    alert(`Selecionado: ${tributo}`);
                };
                menuItems.appendChild(tributoItem);
            });
        }
    }

        function addSubmenusEspecificos(title, submenusList) {
            let menuItems = document.getElementById('menuItems');
            menuItems.innerHTML = '';

            const titleElement = document.createElement('div');
            titleElement.classList.add('menu-title');
            titleElement.innerText = title;
            menuItems.appendChild(titleElement);

            submenusList.forEach(submenu => {
                const submenuItem = document.createElement('div');
                submenuItem.classList.add('menu-item');
                submenuItem.innerText = submenu;

                submenuItem.onclick = () => {
                    alert(`Clicado em ${submenu} em ${title}`);
                };

                menuItems.appendChild(submenuItem);
            });

            menuItems.style.position = 'absolute';
            menuItems.style.top = '80px';
            menuItems.style.left = '10px';
            menuItems.style.display = 'flex';
            menuItems.style.flexDirection = 'column';
        }

        function toggleMainMenu() {
            const menuItems = document.getElementById('menuItems');
            if (menuItems.style.display === 'none' && currentMenu.length === 0) {
                menuItems.style.display = 'flex';
            } else {
                updateMenu('', 0);
            }
        }

        function showSubmenu(element) {
            const submenu = element.querySelector('.submenu');
            if (submenu) {
                submenu.style.display = 'block';

                // If this is a filial (last level before tributos)
                if (element.getAttribute('data-filial')) {
                    const filial = element.getAttribute('data-filial');
                    const empresa = element.getAttribute('data-empresa');
                    const tipo = element.getAttribute('data-tipo');

                    // Create tributos submenu
                    const tributosSubmenu = document.createElement('div');
                    tributosSubmenu.classList.add('submenu');

                    const tributos = submenusTributos["Apuração"][tipo][empresa][filial];
                    tributos.forEach(tributo => {
                        const tributoItem = document.createElement('div');
                        tributoItem.classList.add('menu-item');
                        tributoItem.innerText = tributo;
                        tributoItem.onclick = () => updateMenu(filial, 4, empresa, tipo, tributo);
                        tributosSubmenu.appendChild(tributoItem);
                    });

                    element.appendChild(tributosSubmenu);
                }
            }
        }

        function hideSubmenu(menuItem) {
            const submenu = menuItem.querySelector('.submenu');
            submenu.style.display = 'none';
        }
        function logout() {
        fetch('/logout', {
            method: 'POST',
            headers: {'Content-Type': 'application/json'}
        }).then(response => {
            if (response.redirected) {
                window.location.href = response.url;
            } else {
                alert('Erro ao sair. Tente novamente.');
            }
        }).catch(error => console.error('Erro:', error));
        }
        function updateTributosMenu(empresa, filial, tipo) {
            const menuItems = document.getElementById('menuItems');
            menuItems.innerHTML = '';

            if (submenusTributos["Apuração"][tipo][empresa] &&
                submenusTributos["Apuração"][tipo][empresa][filial]) {

                const tributos = submenusTributos["Apuração"][tipo][empresa][filial];

                const titleElement = document.createElement('div');
                titleElement.classList.add('menu-title');
                titleElement.innerText = `${tipo} - ${empresa} ${filial}`;
                menuItems.appendChild(titleElement);

                tributos.forEach(tributo => {
                    const tributoItem = document.createElement('div');
                    tributoItem.classList.add('menu-item');
                    tributoItem.innerText = tributo;
                    tributoItem.onclick = () => {
                        selectedActivity = {
                            empresa: empresa,
                            filial: filial,
                            tipo: tipo,
                            tributo: tributo
                        };
                        alert(`Selecionado: ${tributo} para ${empresa} ${filial}`);
                    };
                    menuItems.appendChild(tributoItem);
                });
            }
        }

